@model IEnumerable<Graduation.Data.Models.DxfFile>

@{
    ViewData["Title"] = "Index";
}

@section Styles{
    <link href="~/css/Customcss/File.css" rel="stylesheet" />
}
@section Scripts {

}
@*<a asp-action="Create"> Create File</a>*@
<div class="container" id="FilesDiv">
    <div class="row">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">File Name </th>
                    <th scope="col">Date</th>
                    <th scope="col">Edit</th>
                    <th scope="col">Details</th>
                    <th scope="col">Delete</th>
                    <th scope="col">Download</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.FileName</td>
                        <td>@item.UploadedTime</td>
                        <td><a asp-action="Edit" asp-route-id="@item.Id">Edit</a></td>
                        <td><a asp-action="Details" asp-route-id="@item.Id">Details</a></td>
                        <td><a asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
                        <td><a asp-action="Download" asp-route-id="@item.Id">Download</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

